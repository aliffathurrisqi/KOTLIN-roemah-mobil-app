<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alfari.roemahmobilapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@drawable/roemah_mobil_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/roemah_mobil_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".read_databeli"></activity>
        <activity android:name=".input_user"></activity>
        <activity android:name=".read_datatukar"></activity>
        <activity android:name=".data_transaksi" />
        <activity android:name=".tukar_konfirmasi_berhasil" />
        <activity android:name=".beli_konfirmasi_berhasil" />
        <activity android:name=".input_beli" />
        <activity android:name=".input_tukartambah" />
        <activity android:name=".select_invoice" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>